<aiml>

  <!-- Generic Fallback -->
  <category>
    <pattern>*</pattern>
    <template id="tmp_fallback">
      <random>
        <li>I'm sorry but I'm not yet able to understand that command. For a list of commands, type "Help".</li>
      </random>
    </template>
  </category>

</aiml>